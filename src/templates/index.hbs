                            {{/each}}
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</header>
<div class="banner-area banner-skyline banner-horizontal" id="banner" style="background:url(assets/img/bg_header.jpg) center center no-repeat;">
    <div class="container">
        <div class="row">
            <div class="col-sm-8 col-lg-7">
                <div class="banner-text">
                    <div class="banner-table-cell">
                        <h1>
                            {{{banner.header}}}
                        </h1>
                        <p>
                            {{{banner.text}}}
                        </p>
                        <div class="banner-buttons">
                            {{!!--
                            <!-- timetable and signup only mobile visible -->
                            <a href="#missions" class="button-default button-md">Our Missions</a>
                            <a href="#timetable" class="button-default button-md visibile-xs">TimeTable</a>
                            <a href="#missions" class="button-default button-md niantic visibile-xs">Niantic Signup</a>
                            --}}
                        </div>
                    </div>
                </div>
            </div>
