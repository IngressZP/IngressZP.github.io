{{!-- TIME TABLE --}}
<section id="timetable">
    <div class="container clearfix">
        <div class="row">
            <div class="col-md-9 col-md-offset-2">
                <div class="section-title text-center title-blue">
                    <h1>
                        {{{timetable.header}}}
                    </h1>
                    <p class="big text-center">
                        {{timetable.subHeader}}
                    </p>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-9 col-md-offset-2">
                <table class="table timetable text-center">
                    <thead class="text-center">
                        <th>
                            {{timetable.columnHeaders.when}}
                        </th>
                        <th>
                            {{timetable.columnHeaders.what}}
                        </th>
                    </thead>
                    <tbody>
                        {{#each timetable.columns}}
                            <tr>
                                <td>
                                    {{when}}
                                </td>
                                <td>
                                    <strong>{{what.leadText}}</strong><br>
                                    {{what.subText}} <br>
                                    <a class="locationLink"
                                        href="{{what.link}}"
                                        target="_blank" rel="noreferrer nofollow">
                                        [{{../timetable.location}}]
                                    </a>
                                </td>
                            </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</section>

<section id="signup" style="background:url(assets/img/bg_footer.jpg) center  no-repeat;">
       <div class="container clearfix">
            <div class="row">
                <div class="col-md-12">
                    <div class="signup-box wow fadeInUp">
                        <img src="assets/img/md-blank.svg" class="mdlogo">

                        {{#each signup.links}}
                            <a href="{{url}}"
                                target="_blank" rel="noreferrer nofollow"
                                class="btn2">
                                {{text}}
                            </a>
                        {{/each}}
                    </div>
                </div>
            </div>
       </div>
</section>

