			<div class="about-image">
				<img  src="assets/img/promo-city.jpg" alt="Missionday Zaporizhzhia">
			</div>
        </div>
        <div class="col-xs-12 col-sm-7 col-lg-6">
            {{#each intro.text}}
                <p>
                    {{this}}
                </p>
            {{/each}}
        </div>
    </div>
</div>
</section>
